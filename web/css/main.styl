@import 'nib'
@charset "utf-8"

/******* NORMALIZE *******/
@import 'normalize-min'


/* Variables */
color1 = #464B51
color2 = #FF0066

/* Animations */
@keyframes skewbtn
  0% {
    transform: scaleX(1.2)
  }



/******* CSS *******/

html
  height: 100%
  
  
body
  background-attachment scroll
  background-color #E5E5E5
  background-image url("data:image/jpeg;base64,")
  background-repeat repeat
  color #464B51
  font-family 'News Cycle', Verdana, Geneva
  font-size 16px
  height 100%
  margin 0
  padding 0
  text-align center

#container
  color #464B51
  margin 4.5em auto 0
  max-width 1000px
  position relative
  text-align left

#container_bg_0
  background-image url('../img/body_bg_0.jpg')
  background-position left top
  background-repeat repeat-x
  background-attachment scroll
  height 100%
  min-height 100%

#container_bg_1
  background-image url('../img/body_bg_1.jpg')
  background-position center top
  background-repeat repeat-y
  background-attachment scroll
  height auto !important
  min-height 100%

#container_bg_2
  background-color transparent
  background-image url('../img/body_bg_2.jpg')
  background-position center top
  background-repeat no-repeat
  background-attachment scroll
  overflow hidden


footer
  color #464B51
  font-family Oswald, sans-serif
  font-weight 300
  margin 5% auto 2%
  max-width 900px
  text-align center
  -webkit-text-stroke .1px

  p
    margin-top: 0

  #shareme
    background-attachment scroll
    background-image url('../img/footer_bg.jpg')
    background-position center top
    background-repeat no-repeat
    max-height: 240px
    width: 100%

    h2
      font-size 2.7em
      margin 0
      padding 3% 0 1%

      a
        color rgba(255, 255, 255, .6)
        text-decoration none
        &:hover
          color rgba(255, 255, 255, 1)

    .social
      a
        display inline-block
        margin-bottom 10%
        margin-right 3%
        vertical-align middle
        transition transform .15s ease
        &:hover
          transform scale(1.2)

      .email
        background url('../img/contact_sprite.png') no-repeat
        background-position 0px 0px
        height 73px
        width 72px
        &:hover
          background-position -72px 0px

      .linkedin
        background url('../img/contact_sprite.png') no-repeat
        background-position 0px -73px
        height 65px
        width 64px
        &:hover
          background-position -64px -73px

      .twitter
        background url('../img/contact_sprite.png') no-repeat
        background-position 0px -138px
        height 65px
        width 64px
        &:hover
          background-position -64px -138px



header
  color #464B51
  margin-bottom 4%
  overflow hidden

  h1
    border-bottom solid 1px transparent
    font-family 'News Cycle', Verdana, Geneva, sans-serif
    font-size 4.2em
    font-weight 400
    margin 0 auto
    padding-bottom .2em
    text-align center
    transition all .3s ease
    width 70%

    a
      color #464B51
      text-decoration none

    &:hover
      border-bottom solid 1px #DBDBDB

  #designs
    color #7A818A
    font-size 1.8em
    font-weight 400
    letter-spacing 5px
    margin 0
    text-align center
    text-transform uppercase
    -webkit-text-stroke 0.4px #999

h2
  font-family Cookie
  font-size 3.8em
  font-weight 400


nav
  background rgb(101,104,109)
  left 0
  opacity .9
  position fixed
  top 0
  width 100%
  z-index 1000

  ul
    list-style-type none
    margin .5em 0
    padding 0
    text-align center
    width 100%

    li
      border-bottom 1px solid rgba(255,255,255, 0)
      color #fff
      display inline-block
      font-family Oswald
      font-weight 400
      font-size 1em
      margin 0 1em
      padding 0
      text-transform uppercase
      transition all .25s ease
      &:hover
        border-bottom 1px solid rgba(255,255,255, 1)
      a
        color #fff
        text-decoration none



#aboutme
  box-sizing border-box
  padding 0 2%

  .info, .photos, .skills
    box-sizing border-box
    display inline-block
    vertical-align middle

  .info
    width 43%
    margin-left 6%

    h2
      height 60px
      line-height 60px
      margin 0 0 .5em

    p
      font-size 1.1em
      margin 0 0 1.5em
      text-shadow 1px 1px #f9f9f9
      -webkit-text-stroke 0.3px #999

    .contactinfo
      color #FF0066
      -webkit-text-stroke 0.3px #ff76ad

  .photos
    width 32%

    .shadow
      background #fff
      border 1px solid rgba(89,96,102, .3)
      box-sizing border-box
      box-shadow .1em .1em .2em .1em rgba(89,96,102, .2)
      height auto
      position relative
      width 100%
      img
        display block
        margin 2%
        width 96%
      &::before
        box-shadow 0 0 .2em .1em rgba(89,96,102, .3) inset
        content ''
        display block
        height 94%
        margin 2%
        position absolute
        width 96%

    #photo01
      transform rotate(-12deg);
      z-index 2
    #photo02
      margin-top -1em
      transform rotate(12deg)

  .skills
    font-family Rokkitt, serif
    list-style-type none
    margin-left 2%
    padding 0
    width 16%

    h3
      font-size 1.8em
      font-weight 400
      margin 0 0 .5em
      text-shadow 1px 2px #f9f9f9

    li
      margin-bottom .9em
      text-shadow 1px 1px #f9f9f9
      .bar
        background-color #7A818A
        border-radius 3px
        height 6px
        width 100%
        .level
          background-color #FF9DC4
          border-radius 3px
          height 100%
          width 10%


#contact
  box-sizing border-box
  padding 0 1%

  h2
    margin 0 0 .4em

  form, #info
    display inline-block
    vertical-align top

  form
    margin-right 5%
    width 60%
    &::after
      display block
      clear both

    input[type="text"], label, textarea
      display block

    input[type="text"], textarea
      background rgba(255,255,255, 1)
      border solid 1px #ccc
      box-sizing border-box
      margin-bottom .8em
      padding .2em .5em
      width 100%

    label
      color color2
      font-size 1.3em
      margin-bottom .2em
      padding-left .1em

    textarea
      min-height 5em

    #name, #phone
      display inline-block

    #name
      margin-right 5%
      width 60%

    #phone
      width 34%

    #send
      text-align right

      input[type="submit"]
        background color2
        border none
        color #fff
        display inline-block
        font-size 1.2em
        font-weight 700
        letter-spacing .8px
        padding .1em .8em .2em
        text-align center

  #info
    background rgba(255,255,255, .5)
    border solid 1px #ccc
    box-sizing border-box
    padding 1.5em 2em
    width 34%

    h3
      border-bottom solid 1px #D3D4D5
      color color2
      font-size 1.7em
      font-weight 400
      margin 0 0 .6em
      padding-bottom .3em

    dl
      font-size 1.2em

      dt
        font-weight 700
      dd
        margin 0 0 .5em
        &:last-child
          margin 0



#home
  box-sizing border-box
  padding 0 1%

  article
    background #fff
    border 1px solid rgba(89,96,102, .3)
    box-sizing border-box
    box-shadow 0 0 .2em .1em rgba(89,96,102, .15)
    display inline-block
    margin-right 4%
    overflow hidden
    width 30%
    &:last-child
      margin-right 0

    a
      color color1
      text-decoration none
      transition all .15s ease
    &:hover
      a
        color color2
      

  .info

    h2
      font-size 2.5em
      margin .3em 0
      padding 0
      text-align center
      transition all .3s ease
      -webkit-text-stroke 0.3px #666
    p
      display none

  .shadow
    position relative
    margin 4%
    width 92%
    img
      display block
      width 100%
    &::before
      box-shadow 0 0 .2em .1em rgba(89,96,102, .3) inset
      display block
      content ''
      height 100%
      position absolute
      width 100%


#services
  
  article
    background #fff
    border 1px solid #C8C9CB
    box-sizing border-box
    box-shadow .4em .4em #91979E
    display inline-block
    margin-right 4%
    padding 1em 2em 1.4em
    vertical-align top
    width 30%
    -webkit-text-stroke .2px
    &:nth-child(4)
      margin-right 0

    h3
      color color2
      border-bottom 1px solid #D3D4D5
      font-family 'News Cycle'
      font-size 1.8em
      font-weight 400
      margin 0 0 .5em
      padding-bottom .2em
      text-transform capitalize
      
    
    ul
      color color1
      margin 0
      padding 0
      li
        font-size 1.1em
        list-style-type:none
        margin-bottom .4em
        &::before
          content:'- '
        &:last-child
          margin-bottom 1em

    .priceContainer
      border-top 1px solid #D3D4D5
      height 1%
      padding-top 1em
      &::after
        clear both
        content ' '
        display block

      a
        float right
      
      .price
        color color2
        float left
        font-size 2em
        line-height .9em
      

  h2
    height 60px
    line-height 60px
    margin 0 0 .4em

  #personalized
    border 4px solid #FF9DC4
    box-shadow none
    display block
    margin 2.5em auto 0
    width 99%
    h3, p
      display inline-block
      vertical-align middle
    h3
      border 0
      margin 0
      width 6em
    p
      font-size 1.2em
      margin 0
      width 75%
    .priceContainer
      margin-top 1em



    
strong
  font-weight normal

.block
  display block

.bold
  font-weight bold

.btn1
  color #fff
  cursor pointer
  background linear-gradient(top, #FF0066 50%, #FF6FA8)
  border 0
  border-radius .3em
  font-family Oswald
  padding .3em .6em
  text-decoration none
  text-transform uppercase
  transition all .2s ease
  &:hover
    background linear-gradient(top, #FF6FA8 50%, #FF0066)
    animation skewbtn .4s ease

.center
  text-align center

.clear
  clear both

.firstcolor
  color color1
 
.floatl
  float left

.floatr
  float right

.hidden
  overflow hidden
 
.marginR0
  margin-right 0 !important

.notice
  background #FFFF99
  border solid 1px #FBBD04
  box-sizing border-box
  color #724A27
  padding .5em 1em
  width 100%

.relative
  position relative

.secondcolor
  color color2



/******* RESPONSIVE *******/
@import 'responsive';
